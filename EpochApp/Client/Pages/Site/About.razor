@page "/About"
<PageTitle>@StaticUtils.Constants.AppName - About</PageTitle>
<FlatTypo Color="FlatColor.Blue" Typo="Typo.Heading3" Text="About" />

@foreach (var item in AboutContents)
{
	<FlatTypo Typo="Typo.Body1">
		@((MarkupString)item.Content)
	</FlatTypo>
	<FlatBreak BreakType="BreakType.Short" Color="FlatColor.Accent" />
}